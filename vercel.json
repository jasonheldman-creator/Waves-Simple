{
  "buildCommand": "npm run build",
  "rootDirectory": "site",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "wavesintelligence.app"
        }
      ],
      "destination": "https://www.wavesintelligence.app/:path*",
      "permanent": true
    }
  ]
}
